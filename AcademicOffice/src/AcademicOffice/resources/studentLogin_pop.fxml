<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/2.2" fx:controller="academicoffice.LoginController">
    <children>
        <Label layoutX="68.0" layoutY="34.0" text="Username"/>
        <Label layoutX="68.0" layoutY="100.0" text="Password"/>
        <Button fx:id="logInButton" layoutX="413.0" layoutY="165.0" mnemonicParsing="false" onAction="#logIn" text="Log in"/>
        <Label layoutX="68.0" layoutY="68.0" text="Identifiction"/>
        <TextField fx:id="userNameTextField" layoutX="157.0" layoutY="34.0" prefWidth="200.0"/>
        <TextField fx:id="idTextField" layoutX="157.0" layoutY="65.0" prefWidth="200.0"/>
        <PasswordField fx:id="passTextField" layoutX="157.0" layoutY="94.0" prefWidth="200.0"/>
        <Label id="invalidPassLogIn" fx:id="invalidStudent" layoutX="157.0" layoutY="157.0" prefWidth="93.0" text="" textFill="RED"/>
        <Label layoutX="132.0" layoutY="129.0" text="dont have an account yet?"/>
        <Hyperlink fx:id="signupHyperLink" layoutX="284.0" layoutY="126.0" onAction="#signUpWindowPopup" text="sign up!" underline="true">
            <font>
                <Font name="System Italic" size="12.0"/>
            </font>
        </Hyperlink>
    </children>
</AnchorPane>
